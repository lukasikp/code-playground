{
  "name": "code-playground",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "react-guides",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "ts-standard": "^12.0.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "react": "^18.2.0"
  },
  "packageManager": "npm@8.1.0"
}
